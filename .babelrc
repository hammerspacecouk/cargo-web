{
  "presets": [
    "next/babel",
    "@zeit/next-typescript/babel",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3.1.3"
      }
    ]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    ["styled-components", { "ssr": true }]
  ]
}
